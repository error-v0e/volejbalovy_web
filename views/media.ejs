<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@500&display=swap" rel="stylesheet">


    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="/styles/style.css">
    <link rel="stylesheet" href="/styles/media.css">
    <title>Vk Teplice</title>
</head>
<body class="d-flex flex-column">
  <nav class="bg_primary_color navbar navbar-expand-md container-fluid" aria-label="Fourth navbar example">
    <div class="container-xl container-fluid">
      <a class="navbar-brand " href="/">
        <img class="logo" src="/img/<%= kluby.logo %>" alt="Logo" height="110">
      </a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarsExample04" aria-controls="navbarsExample04" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse" id="navbarsExample04">
        <ul class="navbar-nav me-auto mb-2 mb-md-0">
          <% kategorie.forEach(item => { %> 
            <% if(item.id_kategorie == id_kategorie) { %>
              <% if(item.id_kategorie == 2){ %>
                <li class="nav-item dropdown">
                  <a class="nav-link dropdown-toggle bg_primary_color active" href="/tymy/U20" id="dropdown04" data-bs-toggle="dropdown" aria-expanded="false"><%= item.nazev %></a>
                  <ul class="dropdown-menu bg_navbar-collapse collapse bg_primary_color" aria-labelledby="dropdown04">
                    <li><a class="dropdown-item active bg_primary_color" aria-current="page" href="/tymy/U20">U-20</a></li>
                    <li><a class="dropdown-item bg_primary_color" href="/tymy/U20">U-16</a></li>
                    <li><a class="dropdown-item bg_primary_color" href="/tymy/U20">U-14</a></li>
                  </ul>
                </li>
              <% } else{ %>
                <li class="nav-item">
                  <a class="nav-link active primary_color" aria-current="page" href="<%= item.href %>"><%= item.nazev %></a>
                </li>
              <% } %>
            <% } else if(item.id_kategorie == 2){ %>
              <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle bg_primary_color" href="/tymy/U20" id="dropdown04" data-bs-toggle="dropdown" aria-expanded="false"><%= item.nazev %></a>
                <ul class="dropdown-menu bg_navbar-collapse collapse bg_primary_color" aria-labelledby="dropdown04">
                  <li><a class="dropdown-item bg_primary_color" href="/tymy/U20">U-20</a></li>
                  <li><a class="dropdown-item bg_primary_color" href="/tymy/U20l">U-16</a></li>
                  <li><a class="dropdown-item bg_primary_color" href="/tymy/U20">U-14</a></li>
                </ul>
              </li>
            <% } else { %>
              <li class="nav-item">
                <a class="nav-link primary_color" aria-current="page" href="<%= item.href %> "><%= item.nazev %></a>
              </li>
            <% } %>
          <% }); %>
        </ul>
      </div>
    </div>
  </nav>

      <div class="container mt-3">
        <div class="row align-items-stretch mx-auto container-xl media">
          <div class=""> <!--Media-->
            <div class="dropdown">
              <button class="btn mb-4 bg_primary_color" onclick="showFilterMenu()" type="button">
                Filtr
              </button>
            </div>

            <div class="filter-box bg_primary_color">
              <form class="x-button" action="./mediaLoad.js">
                <button class="btn p-0 px-1 pl bg_primary_color" onclick="showFilterMenu()" type="submit">
                  <i class="fa-solid fa-xmark"></i>
                </button>
              </form>
              
              <div class="form-check ">
                <input class="form-check-input" type="checkbox" value="" id="1">
                <label class="form-check-label" for="1">
                  Default checkbox
                </label>
              </div>
              <div class="form-check">
                <input class="form-check-input" type="checkbox" value="" id="2">
                <label class="form-check-label" for="2">
                  Default checkbox
                </label>
              </div>
              <div class="form-check">
                <input class="form-check-input" type="checkbox" value="" id="3">
                <label class="form-check-label" for="3">
                  Default checkbox
                </label>
              </div>
            </div>

            <% prispevky.forEach(item => { %> 
              <div class=""> <!--prispevek-->
                <div class="row g-0 overflow-hidden flex-md-row mb-4 h-md-250 position-relative">
                  <div class="col-auto me-4 mb-3 d-lg-block">
                    <img class="img-fluid" src="/img/<%= item.cesta %>" alt="Foto" width="800px">
                  </div>
                  <div class="col d-flex flex-column position-static caption-media">
                    <h2 class="mb-2 title-media"><%= item.nadpis %></h2>
                    <p class="caption-media-p "><%= item.popisek %></p>
                  </div>
                </div>
              </div>
            <% }); %>



            <div class=""> <!--prispevek-->
              <a href="#" class="row mb-5 g-0 overflow-hidden flex-md-row mb-4 h-md-250 align-items-center position-relative media-link">
                <i class="fa-solid fa-3x primary_color fa-angle-down"></i>
              </a>
            </div>
            
          </div>
          
        </div>
      </div>
      
      <div class="container px-4 py-5 pb-5 mb-3 mt-4 pt-4 ">
        <div class="row row-cols-1 row-cols-lg-3 align-items-stretch align-items-lg-center align-items-md-center g-4 py-5 pt-0  sponzors-container bordert">
          
          <% sponzori.forEach(item => { %> 
            <a class="col sponzors" target=”_blank” href="<%= item.odkaz %>">
              <img class="img-fluid sponzor" src="./img/<%= item.logo %>" alt="sponzor">
            </a>
          <% }) %>

        </div>
      </div>

      <footer class="footer py-2 pt-3 bg_secondary_color">
        <div class="footer-container">
          <a class="secondary_color" href="./index.html">Domu</a>
          <a class="secondary_color" href="./nabor.html">Nabor</a>
          <a class="secondary_color" href="./kontaky.html">Kontakty</a>
          <a class="secondary_color" href="#">Finance</a>
        </div>
        <div class="footer-media-container">
          <a href="https://www.instagram.com/vk.teplice/?hl=cs" target=”_blank” class="fa-brands fa-instagram fa-2xl secondary_color"></a>
          <a href="https://www.facebook.com/vk.teplice/" target=”_blank” class="fa-brands fa-facebook fa-2xl secondary_color"></a>
        </div>
      </footer>
      <script src="./media.js"></script>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</body>
</html>