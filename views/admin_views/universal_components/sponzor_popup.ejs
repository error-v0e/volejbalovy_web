<div class="popup" id="popup<%= item.id_sponzor %>" style="display: none;">
  <form method="post" action="/api/sponzori_popup" enctype="multipart/form-data">
    <div class="action"><!--Ulozit-->
      <a class="butt-dis butt mb-1" href="/sprava">Zahodit zmÄ›ny</a>
      <input  class="butt-sub butt" type="submit" value="Ulozit">
    </div>
    <div class="name primary_color mb-3">
      <label for="odkaz">Odkaz:</label><br>
      <input class="cname primary_color" type="text" id="cname" name="odkaz" value="<%= item.odkaz %>"><br>
    </div>
    <div class="row">
      <div class="mb-2 editeble"> <!--Logo-->
        <input type="file" id="LogoSponzor<%= item.id_sponzor %>" accept="image/*" name="logo" style="display: none;" />
        <img class="editeble previewsponzor" id="previewsponzor<%= item.id_sponzor %>" src="/img/sponzor/<%= item.logo %>" onclick="document.getElementById('LogoSponzor<%= item.id_sponzor %>').click();" alt="Image preview...">
        <i class="fa-solid fa-5x fa-pen pen" onclick="document.getElementById('LogoSponzor<%= item.id_sponzor %>').click();"></i>
        <input type="hidden" name="id" value="<%= item.id_sponzor %>">
      </div>
    </div>
  </form>
</div>